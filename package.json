    "dev": "gulp dev",
    "gulp-debug": "gulp debug",
    "start": "node app/app.js",
    "debug": "nodemon --inspect=0.0.0.0:9229 app/app.js",
    "jshint": "node_modules/jshint/bin/jshint ./app",
    "unit-test": "node specs/unit-tests/testRunner.js -t test",
    "unit-test2": "node node_modules/jasmine/bin/jasmine.js specs/unit-tests/services/setting/index-unitspec.js",
    "deploy-app": "npm run migrate-up && npm run start",
    "karma": "karma start karma.conf.js",
    "bundle-db": "node ./preProcessor/setup/index.js bundle",
    "bundle-and-deploy-db": "node ./preProcessor/setup/index.js bundleAndDeploy",
    "deploy-db": "node ./preProcessor/setup/index.js deploy",
    "migrate-up": "node ./preProcessor/migrate/bin/migrate.js --migration=up",
    "migrate-down": "node ./preProcessor/migrate/bin/migrate.js --migration=down",
    "restore": "node ./preProcessor/setup/index.js restore"
  },
  "author": "teamOpex",
